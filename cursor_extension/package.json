{
  "name": "aistudiocopy",
  "displayName": "ðŸš€ AI Studio Copy Helper",
  "description": "ðŸ“‹ Paste code from clipboard to files - works with path comments",
  "version": "0.2.1",
  "publisher": "furkan",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "clipboard",
    "paste",
    "ai-studio",
    "code",
    "file"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aistudio/copy-helper"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aistudiocopy.pasteToFile",
        "title": "AI Studio: Paste Code to File",
        "icon": "$(file-add)"
      }
    ],
    "configuration": {
      "title": "AI Studio Copy",
      "properties": {
        "aistudiocopy.autoOpenFiles": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open files after creating/updating them"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run esbuild-base -- --minify",
    "watch": "npm run esbuild-base -- --watch",
    "package": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.99.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "esbuild": "^0.25.6"
  }
}
